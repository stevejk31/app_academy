<h1>Log In</h1>

<form action="<%= session_url %>" method="post">
  <%= auth_token %>
  <label name="Name">
    Name
    <input type="text" name="user[name]" value= "<%= @user.name %>">
  </label>

  <label name="Password">
    Password
    <input type="password" name="user[password]">
  </label>

  <input type="submit" value="Log In">
</form>
