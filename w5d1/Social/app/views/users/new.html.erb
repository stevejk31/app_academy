<h1>Sign Up</h1>

<form action="<%= users_url %>" method="post">
  <%= auth_token %>
  <label name="Name">
    Name
    <input type="text" name="user[name]">
  </label>

  <label name="Password">
    Password
    <input type="password" name="user[password]">
  </label>

  <input type="submit" value="Sign Up">
</form>
