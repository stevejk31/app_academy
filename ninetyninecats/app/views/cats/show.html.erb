<h1>
  Cat | <%= @cat.name %>
</h1>
<table border="4">
  <thead>
    <tr>
      <th>Birthday</th>
      <th>Color</th>
      <th>Sex</th>
      <th>Description</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td><%= @cat.birth_date %></td>
      <td><%= @cat.color %></td>
      <td><%= @cat.sex %></td>
      <td><%= @cat.description %></td>
    </tr>
  </tbody>
</table>
<br/>
<body>
<% @cat.cat_rental_requests.order("cat_rental_requests.start_date")
  .each do |rr| %>
  Rented from: <%= rr.start_date %> To: <%= rr.end_date %>
  <br/>
<% end %>
</body>
<br/>
<%= button_to "Edit Cat", edit_cat_url, method: :get%>
<%= button_to "MURDER Cat", cat_url(@cat.id), method: :delete%>
