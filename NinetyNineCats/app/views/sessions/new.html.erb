<h2>Log In!</h2>

<form action="<%= session_url %>" method="post">
  <label for="user[username]">Username:</label>
  <input
    type="text"
    name="user[username]"
    >
  <br/>
  <label for="user[password]">Password</label>
  <input
    type="text"
    name="user[password]"
    >
  <input type="submit" value="submit">
</form>
